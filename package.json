{
  "name": "scan-market",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start-dev": "export SCAN_MODE=basic && npm run config && ng serve",
    "start-fake": "export SCAN_MODE=manual && npm run config && ng serve",
    "start-prod": "npm run build-prod && npm run start",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prety": "prettier --write \"src/**/*.ts\"",
    "config": "ts-node set-env.mjs",
    "start": "npm run config && node server.js",
    "build-prod": "ng build -c production --output-path dist",
    "postinstall": "npm run build-prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.6",
    "@angular/cli": "~9.1.5",
    "@angular/compiler-cli": "~9.1.6",
    "@angular/common": "~9.1.6",
    "@angular/compiler": "~9.1.6",
    "@angular/core": "~9.1.6",
    "@angular/forms": "~9.1.6",
    "@angular/platform-browser": "~9.1.6",
    "@angular/platform-browser-dynamic": "~9.1.6",
    "@angular/router": "~9.1.6",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.5.0",
    "express": "^4.16.3",
    "jsbarcode": "3.11.0",
    "ngx-barcode6": "^1.0.10",
    "node-fetch": "^2.6.0",
    "prettier": "^2.0.5",
    "quagga": "^0.12.1",
    "rxjs": "~6.5.4",
    "scandit-sdk": "^4.6.3",
    "scandit-sdk-angular": "^2.0.7",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.5",
    "@testing-library/angular": "^9.2.2",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "husky": "^4.2.5",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "lint-staged": "^10.2.2",
    "pretty-quick": "^2.0.1",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3",
    "dotenv": "^5.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,css,md}": "prettier --write"
  }
}
